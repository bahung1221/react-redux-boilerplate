{
  "extends": ["react-app"],
  "parserOptions": { "ecmaVersion": 2015 },
  "rules": {
    "no-console": "off",
    "one-var": ["error", {
      "const": "never",
      "let": "consecutive"
    }],
    "semi": ["error", "never"],
    "no-var": "error",
    "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}
